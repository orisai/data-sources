{
	"name": "orisai/data-sources",
	"description": "Load and save data from and to various data formats",
	"keywords": [
		"data",
		"decoder",
		"encoder",
		"files",
		"json",
		"neon",
		"orisai",
		"parser",
		"yaml"
	],
	"type": "library",
	"license": "MPL-2.0",
	"homepage": "https://github.com/orisai/data-sources",
	"authors": [
		{
			"name": "Marek BartoÅ¡",
			"homepage": "https://github.com/mabar"
		}
	],
	"require": {
		"php": ">=7.4.0 <8.2.0",
		"nette/utils": "^3.0.0",
		"orisai/exceptions": "^1.0.0",
		"orisai/utils": "^1.0.0",
		"symfony/polyfill-php80": "^1.20.0"
	},
	"require-dev": {
		"ext-json": "*",
		"brianium/paratest": "^6.3.0",
		"infection/infection": "^0.26.0",
		"nette/neon": "^3.3.1",
		"orisai/coding-standard": "^2.0.0",
		"orisai/nette-di": "^1.0.3",
		"phpstan/extension-installer": "^1.0.0",
		"phpstan/phpstan": "^1.0.0",
		"phpstan/phpstan-deprecation-rules": "^1.0.0",
		"phpstan/phpstan-nette": "^1.0.0",
		"phpstan/phpstan-phpunit": "^1.0.0",
		"phpstan/phpstan-strict-rules": "^1.0.0",
		"phpunit/phpunit": "^9.5.0",
		"staabm/annotate-pull-request-from-checkstyle": "^1.7.0",
		"symfony/yaml": "^5.2.9|^6.0.0"
	},
	"autoload": {
		"psr-4": {
			"Orisai\\DataSources\\": "src"
		}
	},
	"autoload-dev": {
	  "psr-4": {
		"Tests\\Orisai\\DataSources\\": "tests"
	  }
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"sort-packages": true,
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true,
			"infection/extension-installer": true,
			"phpstan/extension-installer": true
		}
	}
}
